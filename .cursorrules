{
  "context": {
    "projectDescription": "Veliano Jewelry E-commerce application specializing in custom grillz, built with Next.js 15, TypeScript, and Supabase",
    "priorityFiles": [
      ".cursor/REVISED-PROJECT-PLAN.md",
      ".cursor/checklists/launch-checklist.md",
      ".cursor/guides/email-verification-implementation.md",
      ".cursor/rules/database-schema-reference.mdc"
    ],
    "currentPhase": "MVP Preparation",
    "remainingFeatures": [
      "Implement email verification with Resend",
      "Complete purchase flow testing",
      "Finalize custom order process",
      "Polish mobile responsiveness",
      "Deploy to production"
    ],
    "completedFeatures": [
      "Core e-commerce functionality",
      "Product management and display",
      "Shopping cart and checkout",
      "Basic authentication flow"
    ]
  },
  "phaseConfig": {
    "email": {
      "priority": 1,
      "dependencies": ["auth"],
      "components": ["EmailVerification", "ResetPassword"],
      "schema": ["auth.users", "profiles"],
      "apis": ["POST /api/auth/verify", "POST /api/auth/reset-password"]
    },
    "checkout": {
      "priority": 2,
      "dependencies": ["products", "auth", "cart"],
      "components": ["CheckoutForm", "OrderConfirmation", "PaymentForm"],
      "schema": ["orders", "order_items"],
      "apis": ["POST /api/checkout", "GET /api/orders"]
    },
    "customOrders": {
      "priority": 3,
      "dependencies": ["auth"],
      "components": ["CustomOrderForm", "DesignUpload", "OrderTracking"],
      "schema": ["custom_orders", "design_uploads"],
      "apis": ["POST /api/custom-orders", "GET /api/custom-orders/[id]"]
    }
  },
  "codeStandards": {
    "fileStructure": {
      "components": "app/components/[feature]/[ComponentName].tsx",
      "serverActions": "app/actions/[feature].ts",
      "repositories": "app/repositories/[name]Repository.ts",
      "types": "types/[feature].ts"
    },
    "typeChecks": {
      "enforceTypedProps": true,
      "enforceTypedReturnValues": true,
      "useZodForValidation": true
    },
    "errorHandling": {
      "useCustomErrorTypes": true,
      "wrapDatabaseErrors": true,
      "provideUserFriendlyMessages": true
    },
    "analytics": {
      "trackPageViews": true,
      "trackUserActions": true,
      "anonymizeData": true
    }
  },
  "mvpLaunch": {
    "deadline": "2 weeks",
    "priorityFeatures": [
      "Email verification",
      "Purchase flow",
      "Custom orders",
      "Mobile responsiveness"
    ],
    "deferredFeatures": [
      "Reviews system",
      "Wishlist functionality",
      "Email marketing",
      "Product recommendations",
      "Advanced search"
    ],
    "checklistFile": ".cursor/checklists/launch-checklist.md"
  }
} 